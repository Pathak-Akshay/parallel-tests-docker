<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Module A Test Suite" verbose="1" parallel="methods" thread-count="3">
    <listeners>
        <listener class-name="com.example.utils.TestListener"/>
    </listeners>

    <test name="Module A Tests">
        <groups>
            <run>
                <include name="moduleA"/>
            </run>
        </groups>
        <classes>
            <class name="com.example.tests.ModuleATests"/>
        </classes>
    </test>
</suite>